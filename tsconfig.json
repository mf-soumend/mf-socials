{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": ["./*"],
      "theme": ["./app/theme"],
      "theme/*": ["./app/theme/*"],
      "settings": ["./settings"],
      "assets/*": ["./assets/*"],
      "screens": ["./app/screens"],
      "components/*": ["./app/components/*"],
      "navigation":["./app/navigation"],
      "utils":["./app/utils"],
      "validation":["./app/validationSchema"],
      "store":["./app/store"],
      "slices/*":["./app/store/slices/*"],
    }
  },
}
